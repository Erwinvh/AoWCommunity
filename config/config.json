      "components": [
        {
          "entrypoint": "./lib/webparts/topBarMembers/TopBarMembersWebPart.js",
          "manifest": "./src/webparts/topBarMembers/TopBarMembersWebPart.manifest.json"
        }
      ]
    },
    "tbd-container-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tbdContainer/TbdContainerWebPart.js",
          "manifest": "./src/webparts/tbdContainer/TbdContainerWebPart.manifest.json"
        }
      ]
    },
    "explore-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/explore/ExploreWebPart.js",
          "manifest": "./src/webparts/explore/ExploreWebPart.manifest.json"
        }
      ]
    },
    "my-writings-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myWritings/MyWritingsWebPart.js",
          "manifest": "./src/webparts/myWritings/MyWritingsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "MemberBarWebPartStrings": "lib/webparts/memberBar/loc/{locale}.js",
    "DocumentFeedbackWebPartStrings": "lib/webparts/documentFeedback/loc/{locale}.js",
    "TopBarWebPartStrings": "lib/webparts/topBar/loc/{locale}.js",
    "FooterWebPartStrings": "lib/webparts/footer/loc/{locale}.js",
    "HomeContainerWebPartStrings": "lib/webparts/homeContainer/loc/{locale}.js",
    "TopBarMembersWebPartStrings": "lib/webparts/topBarMembers/loc/{locale}.js",
    "TbdContainerWebPartStrings": "lib/webparts/tbdContainer/loc/{locale}.js",
    "ExploreWebPartStrings": "lib/webparts/explore/loc/{locale}.js",
    "MyWritingsWebPartStrings": "lib/webparts/myWritings/loc/{locale}.js"
  }
}
